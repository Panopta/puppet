{
  "name": "panopta-panopta",
  "version": "1.1.0",
  "author": "Siebren Zwerver",
  "license": "Apache-2.0",
  "summary": "Official Panopta module",
  "source": "https://github.com/panopta/puppet",
  "project_page": "https://github.com/panopta/puppet",
  "issues_url": "https://github.com/panopta/puppet/issues",
  "tags": ["panopta","monitoring","alerting","reporting"],
  "operatingsystem_support": [
    {
